<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marilia.workout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.marilia.workout.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.marilia.workout.AddExerciseActivity"
            android:label="@string/title_activity_add_exercise"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.marilia.workout.InfoActivity" />
        <activity android:name=".ShowDataActivity"></activity>
    </application>

</manifest>